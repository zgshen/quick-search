{
    "manifest_version": 3,
    "name": "Custom Search Extension",
    "version": "1.0",
    "description": "Custom search engines context menu",
    "permissions": [
      "contextMenus",
      "storage",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }